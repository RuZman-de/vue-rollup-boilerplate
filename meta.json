{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "label": "Project name"
    },
    "description": {
      "type": "string",
      "required": false,
      "label": "Project description",
      "default": "A Vue.js project"
    },
    "author": {
      "type": "string",
      "label": "Author"
    },
    "router": {
      "type": "confirm",
      "label": "Do you want install vue-router?"
    },
    "vuex": {
      "type": "confirm",
      "label": "Do you want install vuex?"
    },
    "vuetify": {
      "type": "confirm",
      "label": "Do you want install vuetify?"
    }
  },
  "filters": {
    "vuetify.js": "vuetify",
    "HelloWorld.vue": "vuetify",
    "logo.svg": "vuetify"
  },
  "completeMessage": "To get started:\n\n  cd {{destDirName}}\n  npm install\n  npm run dev"
}
